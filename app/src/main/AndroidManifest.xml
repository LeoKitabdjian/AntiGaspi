        android:supportsRtl="true"
        android:theme="@style/Theme.AntiGaspi"
        tools:ignore="MissingClass">
        <activity
            android:name=".mainActivities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AjoutIngredientFrigo" />
        <activity android:name=".mainActivities.RechercheAstuce" />
        <activity android:name=".Astuce" />
        <activity android:name=".mainActivities.RecetteMagique" />
        <activity android:name=".Recette" />
        <activity android:name=".ModifListeIngredient" />
        <activity android:name=".mainActivities.MonFrigo" />
        <activity android:name=".mainActivities.RechercheAssociation" />
        <activity android:name=".PhotoIngredient" />
